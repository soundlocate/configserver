java -jar soundInput.jar
    outPort
    -s samplerate
    -d deviceName
    -n numMics
soundSimulate
    outPort
    inPort
    -s samplerate
    -f soundFile
    -l logfile -> wenn dieser Parameter nicht angegeben ist, ist loggign inaktiv
    -p positionfile.csv
soundFFT
    inPort
    outport
    -t sendThreshold
    -f fftSize
    -s samplerate
    -r fftPerSec
    -w windowingFunction
soundLocate
    inPort
    outPort
    -w wsPort
    -d dataAlgorithms
    -a accuracy
    -c maxClusterSize
    -k maxKeep
    -m meanWindow
    -l logfile -> wenn dieser Parameter nicht angegeben ist, ist loggign inaktiv
    -p positionfile.csv